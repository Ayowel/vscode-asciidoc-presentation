{
	"name": "vscode-asciidoc-slides",
	"displayName": "AsciiDoc Slides",
	"description": "Create a presentation via AsciiDoc",
	"version": "0.5.0",
	"publisher": "flobilosaurus",
	"repository": {
		"type": "git",
		"url": "https://github.com/flobilosaurus/vscode-asciidoc-slides.git"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"icon": "media/icon.png",
	"keywords": [
		"asciidoc",
		"reveal.js",
		"slides",
		"presentation"
	],
	"activationEvents": [
		"onCommand:asciidocSlides.preview",
		"onCommand:asciidocSlides.export"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "asciidocSlides.preview",
				"title": "Asciidoc Slides: Show Slides Preview Beside",
				"category": "Asciidoc Slides"
			},
			{
				"command": "asciidocSlides.export",
				"title": "Asciidoc Slides: Export slides as Html file",
				"category": "Asciidoc Slides"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"pretest": "tsc -p ./",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/html-validator": "^5.0.0",
		"@types/mocha": "^7.0.1",
		"@types/node": "^13.9.2",
		"@types/vscode": "^1.41.0",
		"@typescript-eslint/eslint-plugin": "^2.19.0",
		"@typescript-eslint/parser": "^2.19.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"html-validator": "^5.0.0",
		"mocha": "^7.0.1",
		"ts-loader": "^6.2.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@asciidoctor/reveal.js": "^4.0.1",
		"@types/chai": "^4.2.8",
		"@types/cheerio": "^0.22.16",
		"asciidoctor": "^2.1.1",
		"asciidoctor-kroki": "^0.7.0",
		"chai": "^4.2.0",
		"cheerio": "^1.0.0-rc.3",
		"reveal.js": "^3.9.2",
		"slash": "^3.0.0"
	}
}
