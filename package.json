{
	"name": "vscode-asciidoc-slides",
	"displayName": "AsciiDoc Slides",
	"description": "Create a presentation via AsciiDoc",
	"version": "1.3.0",
	"publisher": "flobilosaurus",
	"repository": {
		"type": "git",
		"url": "https://github.com/flobilosaurus/vscode-asciidoc-slides.git"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"icon": "media/icon.png",
	"keywords": [
		"asciidoc",
		"reveal.js",
		"slides",
		"presentation"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "asciidocSlides.preview",
				"title": "Asciidoc Slides: Show Slides Preview Beside",
				"category": "Asciidoc Slides"
			},
			{
				"command": "asciidocSlides.openInBrowser",
				"title": "Asciidoc Slides: Open Slides in Browser",
				"category": "Asciidoc Slides"
			},
			{
				"command": "asciidocSlides.exportHtml",
				"title": "Asciidoc Slides: Export slides to Html file",
				"category": "Asciidoc Slides"
			},
			{
				"command": "asciidocSlides.exportInlinedHtml",
				"title": "Asciidoc Slides: Export slides to inlined Html file",
				"category": "Asciidoc Slides"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"pretest": "tsc -p ./",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.3",
		"@types/node": "^20.8.10",
		"@types/vscode": "^1.75.0",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"eslint": "^8.53.0",
		"glob": "^10.3.10",
		"mocha": "^10.2.0",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.2",
		"vscode-test": "^1.3.0",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@asciidoctor/reveal.js": "^5.0.1",
		"@types/axios": "^0.14.0",
		"@types/chai": "^4.3.9",
		"@types/koa": "^2.13.10",
		"@types/koa-ejs": "^4.3.2",
		"@types/koa-favicon": "^2.1.2",
		"@types/koa-logger": "^3.1.4",
		"@types/koa-send": "^4.1.5",
		"@types/slash": "^3.0.0",
		"@types/web-resource-inliner": "^4.2.4",
		"@types/ws": "^8.5.8",
		"asciidoctor": "^3.0.0",
		"asciidoctor-kroki": "^0.18.1",
		"axios": "^1.6.0",
		"chai": "^4.3.10",
		"font-awesome": "^4.7.0",
		"highlight.js": "^11.9.0",
		"koa": "^2.14.2",
		"koa-easy-ws": "^2.1.0",
		"koa-ejs": "^4.3.0",
		"koa-favicon": "^2.1.0",
		"koa-logger": "^3.2.1",
		"koa-send": "^5.0.1",
		"open": "^9.1.0",
		"reveal.js": "^5.0.1",
		"slash": "^5.1.0",
		"web-resource-inliner": "^6.0.1"
	}
}
